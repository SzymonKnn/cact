    "prettier": "^2.8.8",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^4.2.2",
    "@salesforce-ux/design-system": "=2.21.2",
